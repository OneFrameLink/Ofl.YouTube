<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <Description>Functionality for interacting with YouTube APIs.</Description>
    <TargetFramework>netstandard1.3</TargetFramework>
    <AssemblyName>Ofl.YouTube</AssemblyName>
    <PackageId>Ofl.YouTube</PackageId>
    <PackageProjectUrl>https://github.com/OneFrameLink/Ofl.YouTube</PackageProjectUrl>
    <RepositoryUrl>https://github.com/OneFrameLink/Ofl.YouTube.git</RepositoryUrl>
	<Version>2.0.0</Version>
	<PackageTags>google google-data-api google-api</PackageTags>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.AspNetCore.WebUtilities" Version="1.1.2" />
    <PackageReference Include="Microsoft.Extensions.Primitives" Version="1.1.1" />
    <PackageReference Include="Microsoft.NETCore.Portable.Compatibility" Version="1.0.1" />
    <PackageReference Include="Ofl.Collections" Version="1.0.3" />
    <PackageReference Include="Ofl.Google" Version="2.0.3" />
    <PackageReference Include="Ofl.Net.Http.Abstractions" Version="1.0.5" />
    <PackageReference Include="Ofl.Primitives" Version="1.1.4" />
    <PackageReference Include="Ofl.Threading.Tasks" Version="1.0.2" />
    <PackageReference Include="Ofl.YouTube.Contracts" Version="1.0.3" />
    <PackageReference Include="System.Threading.Tasks.Extensions" Version="4.3.0" />
  </ItemGroup>

  <ItemGroup>
    <Folder Include="Properties\" />
  </ItemGroup>

</Project>
